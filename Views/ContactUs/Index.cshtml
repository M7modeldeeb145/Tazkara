@using Tazaker.Models
@model List<ContactUs>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>
@foreach(var contactus in Model)
{
    <div>
        <h4>ContactUs</h4>
        <hr />
        <dl class="row">
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => contactus.Name)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => contactus.Name)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => contactus.Email)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => contactus.Email)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => contactus.Message)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => contactus.Message)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => contactus.Subject)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => contactus.Subject)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => contactus.Status)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => contactus.Status)
            </dd>
        </dl>
    </div>
    <div>
        <a asp-action="Edit" asp-route-id="@contactus?.Id">Edit</a> |
        <a asp-action="Index">Back to List</a>
    </div>
}

